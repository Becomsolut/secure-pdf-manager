{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Secure PDF Manager by Becomsolut",
  "version": "0.1.1",
  "identifier": "com.becomsolut.securepdf",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "plugins": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/Becomsolut/secure-pdf-manager.git"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDY2MkY3NkVFMTRGRDJBNUYKUldSZkt2MFU3bll2WnE4blVRcjRxQUVxU3M5SVBURWx3VmtlYXlYc3NzUHZsRFFaTnBBdldJYnUK"
    }
  },
  "app": {
    "windows": [
      {
        "title": "Secure PDF Manager by Becomsolut",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}